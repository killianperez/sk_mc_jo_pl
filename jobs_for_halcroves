									set {_Shear2::*} to configuration section "Shear.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									set {_Shear3} to single value "Shear.%loop-value%.%{_Shear2::1}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									set {_Shear4} to single value "Shear.%loop-value%.%{_Shear2::2}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									send "&8[&3JOBS&8] &4%{_loop.number}% &7- Teindre Mouton - &6%loop-value% &7--> &6%{_Shear3}%$ &7--> &6%{_Shear4}% xp"
									
								set {_Explore::*} to configuration section "Explore" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
								set {_loop.number} to 0
								loop {_Explore::*}:
									add 1 to {_loop.number}
									set {_Explore2::*} to configuration section "Explore.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									set {_Explore3} to single value "Explore.%loop-value%.%{_Explore2::1}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									set {_Explore4} to single value "Explore.%loop-value%.%{_Explore2::2}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
									send "&8[&3JOBS&8] &4%{_loop.number}% &7- Explorer - &6%loop-value% &7--> &6%{_Explore3}%$ &7--> &6%{_Explore4}% xp"
								send "&8[&3JOBS&8] &2&lFIN DU DIAGNOSTIQUE DE %ARG-2%"
								stop
							if file "plugins/killian6341_config/jobs/jobs/%arg-2%.yml" doesn't exists:
								send "&8[&3JOBS&8] &6Jobs %arg-2% introuvable."
					if arg 1 is "debug":
						if {debug.jobs} is not set:
							set {debug.jobs} to true
							set {_DBG_0X4_BLK_BRK_PLK::*} to value list "BREAKABLE_OR_PLACABLE" get of "plugins/killian6341_config/jobs/aliases/block.yml"
							loop {_DBG_0X4_BLK_BRK_PLK::*}:
								add 1 to {_DBG_0X4_BLK_BRK_PLK_LP_NB}
								send "&6%{_DBG_0X4_BLK_BRK_PLK_LP_NB}% &7- &c%loop-value%"
							set {_DBG_0X4_BLK_BRK_PLK::*} to value list "BREAKABLE_OR_PLACABLE" get of "plugins/killian6341_config/jobs/aliases/block.yml"
							loop {_DBG_0X4_BLK_BRK_PLK::*}:
								add 1 to {_DBG_0X4_BLK_BRK_PLK_LP_NB}
								send "&6%{_DBG_0X4_BLK_BRK_PLK_LP_NB}% &7- &c%loop-value%"
							set {_DBG_0X4_BLK_BRK_PLK::*} to value list "BREW_TYPE" get of "plugins/killian6341_config/jobs/aliases/brew.yml"
							loop {_DBG_0X4_BLK_BRK_PLK::*}:
								add 1 to {_DBG_0X4_BLK_BRK_PLK_LP_NB}
								send "&6%{_DBG_0X4_BLK_BRK_PLK_LP_NB}% &7- &c%loop-value%"
							set {_DBG_0X4_BLK_BRK_PLK::*} to value list "ITEM_REPAIRED" get of "plugins/killian6341_config/jobs/aliases/repair.yml"
							loop {_DBG_0X4_BLK_BRK_PLK::*}:
								add 1 to {_DBG_0X4_BLK_BRK_PLK_LP_NB}
								send "&6%{_DBG_0X4_BLK_BRK_PLK_LP_NB}% &7- &c%loop-value%"
							send "&lFOUND ID : (DBG_0X4_BLK_BRK_PLK_LP_NB): &7&l%{_DBG_0X4_BLK_BRK_PLK_LP_NB}% aliases supported"
							send "&1&lATTENTION : Debug mode actif &7- Les joueurs voient aussi tout ce qui font . Retapez /ajobs debug pour y désactiver."
						else if {debug.jobs} is true:
							delete {debug.jobs}
							send "&1&lATTENTION : Debug mode non actif"
					if arg 1 is "list":
						send "&7======= [ JOBS &c&lADMIN &7] ======="
						set {_jobs_list::*} to files in "plugins/killian6341_config/jobs/jobs"
						loop {_jobs_list::*}:
							add 1 to {_jobs.list.number}
							set {_size.jobs} to size of file "plugins/killian6341_config/jobs/jobs/%{_jobs_list::%{_jobs.list.number}%}%"
							replace all "KB" with "Ko" in {_size.jobs}
							replace all "MB" with "Mo" in {_size.jobs}
							replace all "GB" with "Go" in {_size.jobs}
							replace all "B" with "octets" in {_size.jobs}
							replace all "1.0 octets" with "-1 Ko" in {_size.jobs}
							send "&6%{_jobs.list.number}% &7- &c%loop-value% &7- &6%{_size.jobs}%"
					if arg 1 is "remove":
						if arg 2 is not set:
							send "&7======= [ JOBS &c&lADMIN &7] =======" and "&7/ajobs remove <nom du fichier du jobs>" and " "
						if arg 2 is set:
							if file "plugins/killian6341_config/jobs/jobs/%arg-2%.yml" exists:
								df "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
								send "&8[&4JOBS&8] &6Jobs %arg-2% supprimé."
								stop
							if file "plugins/killian6341_config/jobs/jobs/%arg-2%.yml" doesn't exists:
								send "&8[&4JOBS&8] &6Jobs %arg-2% introuvable."
					if arg 1 is "create":
						if {editor.mode.%player%} is not set:
							send player title "&9JOBS CREATION" with subtitle "&6Pour quitter tapez 'quit'" for 99 second
							wait 4 second
							send player title "&9Étape 1" with subtitle "&6Entrer un nom de métier dans le chat." for 99 second
							set {editor.mode.%player%} to 0
						else:
							send "&4Erreur: &6Mode édition déjà en cours..."
		else:	
			send "&8[&4JOBS&8] &4Erreur: &6Jobs n'est pas actif ici."
on chat:
	if player is not in world "world":
		send "&8[&6JOBS&8] &4Erreur: &cJobs est désactivé ici!."
	if world of player is equal to "world":
		if {editor.mode.%player%} is set:
			cancel event
		if message contain "quit":
			if {editor.mode.%player%} is set:
				delete {editor.mode.%player%}
				delete file "plugins/killian6341_config/jobs/jobs/%{last.name.of.yaml.jobs.%player%}%.yml"
				delete {last.name.of.yaml.jobs.%player%}
				send player title "&9JOBS" with subtitle "&6Vous avez quitter le mode de création." for 1 second
		if {editor.mode.%player%} is 0:
			if file "plugins/killian6341_config/jobs/jobs/%message%" exists:
				send player title "&4&lERREUR" with subtitle "&6Ce job éxiste déjà." for 4 second
				wait 3 second
				send player title "&9Étape 1" with subtitle "&6Entrer un nom de métier dans le chat." for 99 second
			if message contain " " or ".":
				send player title "&4&lERREUR" with subtitle "&6L'espace ou le point n'est pas autorisé." for 4 second
				wait 3 second
				send player title "&9Étape 1" with subtitle "&6Entrer un nom de métier dans le chat." for 99 second
			else:
				set {last.name.of.yaml.jobs.%player%} to message
				send player title "&9Étape 1" with subtitle "&6Copie du fichier..." for 1 second
				copy file "plugins/killian6341_config/jobs/seed/temp.yml" to "plugins/killian6341_config/jobs/jobs/"
				download from "https://drive.google.com/uc?export=download&id=0B2y_lcANJO7tSE9vVDZXNG5PTms" to "plugins/killian6341_config/jobs/jobs/"
				wait 5 tick
				send player title "&9Étape 1" with subtitle "&6Chargement en cours..." for 10 second
				rename file "plugins/killian6341_config/jobs/jobs/temp.yml" to "%{last.name.of.yaml.jobs.%player%}%.yml"
				wait 5 tick
				set "metier" to "%{last.name.of.yaml.jobs.%player%}%" in yaml file "plugins/killian6341_config/jobs/jobs/%{last.name.of.yaml.jobs.%player%}%.yml"
				send player title "&9Étape 1" with subtitle "&6Fichier créer..." for 10 second
				delete {editor.mode.%player%}
				delete {last.name.of.yaml.jobs.%player%}
				send player title "&9LES JOBS" with subtitle "&6plugins/killian6341_config/jobs/jobs" for 3 second
				wait 4 second
				send player title "&9" with subtitle "&2Fin de la création." for 3 second
