on right click on a cow:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_Milk::*} to configuration section "Milk" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
		loop {_Milk::*}:
			add 1 to {_loop.number}
			set {_Milk3} to single value "Milk.%{_Milk::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_Milk4} to single value "Milk.%{_Milk::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_block} to convert text "WH_NONE_SSS_OF_DEBUG_MILK" to uppercase
			replace all " " with "_" in {_block}
			if player is holding a empty bucket:
				set {_Milk3} to {_Milk3} parsed as number
				set {_Milk4} to {_Milk4} parsed as number
				add {_Milk3} to player's balance
				loop {_Milk4} times:
					give experience bottle named "&2&oJOBS récompense" to player
					wait 1 tick
				if {_Milk3} < 0:
					set action bar of player to "&7%{jobs.0X3}% %{_Milk3}%€"
				if {_Milk3} > 0:
					set action bar of player to "&7%{jobs.0X1}% +%{_Milk3}%€"
				if {_Milk3} = 0:
					set action bar of player to "&7%{jobs.0X2}% %{_Milk3}%€"
		if {debug.jobs} is true:
			send "plugin &7DEBUG : &1%{_block}%"
			
