# function createJobsdropHead(p: player, data: text):
	# drop player head with nbt "%{_data}%" {_p}'s location
every 10 second:
	set {jobs.0X1} to single value "ggn_pos" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X2} to single value "ggn_zer" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X3} to single value "ggn_neg" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X41} to single value "allowed_world_1" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X42} to single value "allowed_world_2" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X43} to single value "allowed_world_3" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X44} to single value "allowed_world_4" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X45} to single value "allowed_world_5" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X46} to single value "allowed_world_6" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X47} to single value "allowed_world_7" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X48} to single value "allowed_world_8" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X49} to single value "allowed_world_9" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X41} to {jobs.0X41} parsed as text
	set {jobs.0X410} to single value "allowed_world_10" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X5} to single value "prix_singulier" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X6} to single value "prix_pluriel" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X7} to single value "pric_singulier" get of "plugins/killian6341_config/jobs/config.yml"
	set {jobs.0X8} to single value "pric_pluriel" get of "plugins/killian6341_config/jobs/config.yml"
on place:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_PLACE::*} to configuration section "Place" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
		loop {_PLACE::*}:
			add 1 to {_loop.number}
			set {_PLACE2::*} to configuration section "Place.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_PLACE3} to single value "Place.%loop-value%.%{_PLACE2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_PLACE4} to single value "Place.%loop-value%.%{_PLACE2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_block} to convert text "%block%" to uppercase
			set {_block²} to loop-value
			if {_block²} = {_block}:
				set {_PLACE3} to {_PLACE3} parsed as number
				set {_PLACE4} to {_PLACE4} parsed as number
				add {_PLACE3} to player's balance
				loop {_PLACE4} times:
					give experience bottle named "&2&oJOBS récompense" to player
					wait 1 tick
				if {_PLACE3} < 0:
					set action bar of player to "&7%{jobs.0X3}% %{_PLACE3}%€"
				if {_PLACE3} > 0:
					set action bar of player to "&7%{jobs.0X1}% +%{_PLACE3}%€"
				if {_PLACE3} = 0:
					set action bar of player to "&7%{jobs.0X2}% %{_PLACE3}%€"
			if {debug.jobs} is true:
				send "plugin &7DEBUG MODE PLACE : &1%{_PLACE::*}% &2%{_loop.number}% &3%{_PLACE2::*}% &4%{_PLACE3}% &5%{_PLACE4}% &6%{_block}% &7%{_block²}%"
		stop
on break:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_loc} to location of target block
		set {_loc2} to player's location
		set {_break::*} to configuration section "Break" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
		loop {_break::*}:
			add 1 to {_loop.number}
			set {_break2::*} to configuration section "Break.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_break3} to single value "Break.%loop-value%.%{_break2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_break4} to single value "Break.%loop-value%.%{_break2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_break5} to single value "Break.%loop-value%.%{_break2::3}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_break5} to {_break5} parsed as number
			set {_block} to convert text "%event-block%" to uppercase
			set {_block²} to loop-value
			set {_break7} to line {_break} parsed as item
			set {block} to {_block}
			if {_block²} = {_block}:
				cancel event
				loop {_break5} times:
					set block to {_block}
					drop 1 of block 
					# teleport player to {_loc2}
				set block at {_loc} to air
				delete {block}
				set {_break3} to {_break3} parsed as number
				set {_break4} to {_break4} parsed as number
				add {_break3} to player's balance
				loop {_break4} times:
					give experience bottle named "&2&oJOBS récompense" to player
					wait 1 tick
				add {_break4} to the spawned experience
				if {_break3} < 0:
					set action bar of player to "&7%{jobs.0X3}% %{_break3}%€"
				if {_break3} > 0:
					set action bar of player to "&7%{jobs.0X1}% +%{_break3}%€"
				if {_break3} = 0:
					set action bar of player to "&7%{jobs.0X2}% %{_break3}%€"
			if {debug.jobs} is true:
				send "plugin &7DEBUG MODE break : &1%{_break::*}% &2%{_loop.number}% &3%{_break2::*}% &4%{_break3}% &5%{_break4}% &6%{_block}% &7%{_block²}% &8%{_break5}% &9%event-block%"
		stop
on enchant: 
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_Enchanting::*} to configuration section "Enchanting" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
		loop {_Enchanting::*}:
			add 1 to {_loop.number}
