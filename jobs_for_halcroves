on inventory click:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_inventory.type} to  "%inventory type of clicked inventory%"
		if {_inventory.type} = "WORKBENCH":
			set {_Craft::*} to configuration section "Craft" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			if clicked slot type is RESULT:
				loop {_Craft::*}:
					add 1 to {_loop.number}
					set {_Craft2::*} to configuration section "Craft.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_Craft3} to single value "Craft.%loop-value%.%{_Craft2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_Craft4} to single value "Craft.%loop-value%.%{_Craft2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_block} to convert text "%clicked item%" to uppercase
					set {_block²} to loop-value
					if {_block²} = {_block}:
						set {_Craft3} to {_Craft3} parsed as number
						set {_Craft4} to {_Craft4} parsed as number
						add {_Craft3} to player's balance
						loop {_Craft4} times:
							give experience bottle named "&2&oJOBS récompense" to player
							wait 1 tick
						if {_Craft3} < 0:
							set action bar of player to "&7%{jobs.0X3}% %{_Craft3}%€"
						if {_Craft3} > 0:
							set action bar of player to "&7%{jobs.0X1}% +%{_Craft3}%€"
						if {_Craft3} = 0:
							set action bar of player to "&7%{jobs.0X2}% %{_Craft3}%€"
						stop
		if {_inventory.type} = "FURNACE":
			set {_Smelt::*} to configuration section "Smelt" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			loop {_Smelt::*}:
				if clicked slot type is RESULT:
					add 1 to {_loop.number}
					set {_Smelt2::*} to configuration section "Smelt.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_Smelt3} to single value "Smelt.%loop-value%.%{_Smelt2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_Smelt4} to single value "Smelt.%loop-value%.%{_Smelt2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
					set {_block} to convert text "%clicked item%" to uppercase
					set {_block²} to loop-value
					if {_block²} = {_block}:
						set {_Smelt3} to {_Smelt3} parsed as number
						set {_Smelt4} to {_Smelt4} parsed as number
						add {_Smelt3} to player's balance
						loop {_Smelt4} times:
							give experience bottle named "&2&oJOBS récompense" to player
							wait 1 tick
						if {_Smelt3} < 0:
							set action bar of player to "&7%{jobs.0X3}% %{_Smelt3}%€"
						if {_Smelt3} > 0:
							set action bar of player to "&7%{jobs.0X1}% +%{_Smelt3}%€"
						if {_Smelt3} = 0:
							set action bar of player to "&7%{jobs.0X2}% %{_Smelt3}%€"
						stop
					if {debug.jobs} is true:
						send "plugin &7DEBUG : &1%{_block}% "
		if {debug.jobs} is true:
			send "plugin &7DEBUG : &1%clicked action% &2%clicked inventory% &3%clicked cursor% &4%clicked item% &5%name of clicked item% &6%clicked slot type% &7%clicked type% &8%clicked type number% &9%clicked slot type%"

