			set {_Eat4} to single value "Eat.%loop-value%.%{_Eat2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_block} to convert text "%item%" to uppercase
			set {_block²} to loop-value
			if {_block²} = {_block}:
				set {_Eat3} to {_Eat3} parsed as number
				set {_Eat4} to {_Eat4} parsed as number
				add {_Eat3} to player's balance
				add {_Eat4} to xp of player
