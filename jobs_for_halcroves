			set {_Enchanting3} to single value "Enchanting.%loop-value%.%{_Enchanting2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_Enchanting4} to single value "Enchanting.%loop-value%.%{_Enchanting2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_block} to convert text "%event-item%" to uppercase
			set {_block²} to loop-value
			if {_block²} = {_block}:
				set {_Enchanting3} to {_Enchanting3} parsed as number
				set {_Enchanting4} to {_Enchanting4} parsed as number
				add {_Enchanting3} to player's balance
				loop {_Enchanting4} times:
					give experience bottle named "&2&oJOBS récompense" to player
					wait 1 tick
				if {_Enchanting3} < 0:
					set action bar of player to "&7%{jobs.0X3}% %{_Enchanting3}%€"
				if {_Enchanting3} > 0:
					set action bar of player to "&7%{jobs.0X1}% +%{_Enchanting3}%€"
