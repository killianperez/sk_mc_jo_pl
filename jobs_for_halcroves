			set {_Repair4} to single value "Repair.%loop-value%.%{_Repair2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
			set {_block} to convert text "%event-item-result%" to uppercase
			send "%event-item-result%"
			set {_block²} to loop-value
			if {_block²} = {_block}:
				set {_Repair3} to {_Repair3} parsed as number
				set {_Repair4} to {_Repair4} parsed as number
				add {_Repair3} to player's balance
				loop {_Repair4} times:
					give experience bottle named "&2&oJOBS récompense" to player
