			# set action bar of player to "&7%{jobs.0X1}% +%{_brewing3}%€"
	# stop
on join:
	# Cette variable peux-être supprimé à tout moment.
	set {jobs.a.%player%} to single value "metier" get of "plugins/killian6341_config/jobs/players/%player%.yml"
on quit:
	delete {jobs.a.%player%}
function createTitleErrored(p: player):
	loop 2000 times:
		send {_p} title "&4&lErreur : &c&l%{error_trouved}%" with subtitle "&6%{jobs.error.text}%" for 10 second with 0 tick fade in and 0 tick fade out
		wait 1 tick
on load:
	send "JOBS PLUGIN DÉMARRE.. Merci de patienter..." to console
	if file "plugins/killian6341_config/jobs" doesn't exists:
		if file "plugins/killian6341_config" doesn't exists:
			send "[JOBS] Création du dossier pour les plugins de Killian6341..." to console
			create folder "plugins/killian6341_config"
			send "[JOBS] &2&lTerminé" to console
		send "[JOBS] Création du dossier pour JOBS" to console
		create folder "plugins/killian6341_config/jobs"
		create folder "plugins/killian6341_config/jobs/players"
		create folder "plugins/killian6341_config/jobs/jobs"
		send "[JOBS] &2&lTerminé" to console
	send "[JOBS] &6Démarrage du plugin..." to console
command /jobs [<text>] [<text>]:
	trigger:
		if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
			if player is not in world "world":
				send "&8[&6JOBS&8] &4Erreur: &cJobs est désactivé ici!."
			if world of player is equal to "world":
				if arg 1 is not set:
					if player has permission "jobs.admin":
						send "&cVous avez été détecté en tant qu'un administrateur" and "&cUtilisez donc la commande /ajobs"
					send "&7======= [ JOBS ] ======="
					send "&6/jobs browse &7- &3Liste tout les métiers"
					send "&6/jobs info &7- &3Plus d'informations sur les métiers"
					send "&6/jobs join &7- &3Rejoindre un métier"
					send "&6/jobs leave &7- &3Quitter un métier"
					send ""
					if {jobs.a.%player%} is set:
						send "&7Votre jobs actuel: &6&l%{jobs.a.%player%}%"
					else:
						send "&7Votre jobs actuel: &4&lAUCUN"
				if arg 1 is "leave":
					if file "plugins/killian6341_config/jobs/players/%player%.yml" exists:
						df "plugins/killian6341_config/jobs/players/%player%.yml"
						send "&8[&6JOBS&8] &3Vous n'avez désormais plus de métier."
						stop
					if file "plugins/killian6341_config/jobs/players/%player%.yml" doesn't exists:
						send "&8[&6JOBS&8] &3Vous n'avez déjà pas de métier."
				if arg 1 is "join":
					if arg 2 is not set:
						send "&8[&6JOBS&8] &4Erreur: &cNom du job invalide."
					if arg 2 is set:
						set {_jobs_detect::*} to files in "plugins/killian6341_config/jobs/players"
						replace all ".yml" with "" in {_jobs_detect::*}
						if {_jobs_detect::*} contain "%player%":
							send "&8[&6JOBS&8] &4Erreur: &cQuittez un job d'abord."
							stop
						set {_jobs_list::*} to files in "plugins/killian6341_config/jobs/jobs"
						replace all ".yml" with "" in {_jobs_list::*}
						if {_jobs_list::*} doesn't contain arg-2:
							send "&8[&6JOBS&8] &4Erreur: &cNom du job invalide."
							stop
						create file "plugins/killian6341_config/jobs/players/%player%.yml"
						wf "metier: ""%arg-2%""" to "plugins/killian6341_config/jobs/players/%player%.yml"
						set {jobs.a.%player%} to single value "metier" get of "plugins/killian6341_config/jobs/players/%player%.yml"
						send "&8[&6JOBS&8] &3Vous avez rejoint le métier : %arg-2%."
				if arg 1 is "info":
					if arg 2 is not set:
						send "&4Erreur: &cNom du job invalide."
					if arg 2 is set:
						set {_jobs_list::*} to files in "plugins/killian6341_config/jobs/jobs"
						replace all ".yml" with "" in {_jobs_list::*}
						if {_jobs_list::%arg-2%} don't contain arg-2:
							send "&4Erreur: &cNom du job invalide."
							stop
						open chest with 6 rows named "&lPlus d'infos sur : %arg-2%" to player
						wait 3 tick
						format slot 4 of player with barrier named "&4&lFERMER" to close
						
						
						
						
						
						
						
						
						
						
						
						
						
						
