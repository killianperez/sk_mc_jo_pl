					clear {_hfuifhof}
					
					
						
						
						
							
							
							
							
							
							
							
							
							
							
							
							
				if arg 1 is "browse":
						# set {_jobs_list::*} to directory "plugins/killian6341_config/jobs/jobs"'s files 
					set {_jobs_list::*} to files in "plugins/killian6341_config/jobs/jobs"
					send "&7======= [ JOBS ] ======="
					if {_jobs_list::*} is not set:
						send "&4Erreur: &cAucun métier détecté."
					else if {_jobs_list::*} is set:
						set {_loop.id} to 1
						send "&8Liste de tout les jobs disponibles:"
						loop {_jobs_list::*}:
							if {_jobs_list::%{_loop.id}%} is set:
								replace all ".yml" with " " in {_jobs_list::*}
								send "&c%{_loop.id}% &7- &6%{_jobs_list::%{_loop.id}%}%" to player
								add 1 to {_loop.id}
					send "&7======= [ LISTE ] ======="
		else:	
			send "&8[&4JOBS&8] &4Erreur: &6Jobs n'est pas actif ici."
command /ajobs [<text>] [<text>]:
	trigger:
		if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
			if player is not in world "world":
				send "&8[&6JOBS&8] &4Erreur: &cJobs est désactivé ici!."
			if world of player is equal to "world":
				if player don't have permission "jobs.admin":
					send "&4Erreur: &6Vous n'avez pas la permission."
				else if player have permission "jobs.admin":
					if arg 1 is not set:
						send "&7======= [ JOBS &c&lADMIN &7] ======="
						send "&7/ajobs create &6- Créer un nouveau jobs étape par étape"
						send "&7/ajobs remove &6- Supprimer un jobs"
						send "&7/ajobs list &6- List tout les job avec informations avencés"
						send "&7/ajobs player &6- Configurer un joueur"
						send "&7/ajobs stats &6- Statistiques d'un joueur"
						send "&c/ajobs reset &6- Réinitialiser le plugin"
						send "&2/ajobs test &6- Tester ce que le plugin comprend sur un job."
						send "&2/ajobs compl &6- Vérifiez compatibilité du plugin."
						send "&e/ajobs debug &6- 4DD87D4874C8C77C4C8C7"
						send " "
					if arg 1 is "compl":
						send "&7======= [ DIAGNOSTIQUE JOBS &7] ======="
						set {_plugins::*} to plugins list
						send "&7--> Vérifie si vous possédez tout les plugins JAVA nécessaire."
						if {_plugins::Vault} is not set:
							send "&7&lVault: &2&lAUCUN PROBLEME"
						else:
							send "&7&lVault: &4&lMANQUANT"
						if {_plugins::Skript} is not set:
							send "&7&lSkript: &2&lAUCUN PROBLEME"
						else:
							send "&7&lSkript: &4&lMANQUANT"
						if {_plugins::Skelett} is not set:
							send "&7&lSkelett: &2&lAUCUN PROBLEME"
						else:
							send "&7&lSkelett: &4&lMANQUANT"
