every 4 tick:
	loop all players:
		if world of loop-player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
			set {_loc} to loop-player's location
			wait 3 tick
			set {_loc2} to loop-player's location
			if {_loc} is not equal to {_loc2}:
				# if time in the loop-player's world is greater than 8:00
				set {_Explore::*} to configuration section "Explore" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
				loop {_Explore::*}:
					add 1 to {_loop.number}
					set {_Explore2::*} to configuration section "Explore.%loop-value-2%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
					set {_Explore3} to single value "Explore.%loop-value-2%.%{_Explore2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
					set {_Explore4} to single value "Explore.%loop-value-2%.%{_Explore2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
					replace all " " with "_" in {_block}
					set {_block²} to loop-value-2
					if {_block²} = "BLOCK":
						set {_Explore3} to {_Explore3} parsed as number
						set {_Explore4} to {_Explore4} parsed as number
						set {_Explore31} to {_Explore3}
						add {_Explore31} to loop-player's balance
						loop {_Explore4} times:
							give experience bottle named "&2&oJOBS récompense" to loop-player
							wait 1 tick
						if {_Explore3} < 0:
							set action bar of loop-player to "&7%{jobs.0X3}%&4 %{_Explore3}%€"
						if {_Explore3} > 0:
							set action bar of loop-player to "&7%{jobs.0X1}%&2 +%{_Explore3}%€"
						if {_Explore3} = 0:
							set action bar of loop-player to "&7%{jobs.0X2}%&6 %{_Explore3}%€"
			# if {debug.jobs} is true:
				# send "debug &7%loop-player's location%, w: %yaw of loop-player's location%, p: %pitch of loop-player's location%"
			set {_loc} to chunk at the loop-player
			set {old_chunk_o_%loop-player%} to {_loc}
			wait 3 tick
			set {_loc2} to chunk at the loop-player
			if {_loc} is not equal to {_loc2}:
				if {_loc2} is not equal to {old_chunk_o_%loop-player%}:
					set {_Explore::*} to configuration section "Explore" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
					loop {_Explore::*}:
						add 1 to {_loop.number}
						set {_Explore2::*} to configuration section "Explore.%loop-value-2%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
						set {_Explore3} to single value "Explore.%loop-value-2%.%{_Explore2::1}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
						set {_Explore4} to single value "Explore.%loop-value-2%.%{_Explore2::2}%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%loop-player%}%.yml"
						replace all " " with "_" in {_block}
						set {_block²} to loop-value-2
						if {_block²} = "CHUNK":
							set {_Explore3} to {_Explore3} parsed as number
							set {_Explore4} to {_Explore4} parsed as number
							set {_Explore31} to {_Explore3}
							add {_Explore31} to loop-player's balance
							loop {_Explore4} times:
								give experience bottle named "&2&oJOBS récompense" to loop-player
								wait 1 tick
							if {_Explore3} < 0:
								set action bar of loop-player to "&4%{jobs.0X3}% %{_Explore3}%€"
							if {_Explore3} > 0:
								set action bar of loop-player to "&2%{jobs.0X1}% +%{_Explore3}%€"
							if {_Explore3} = 0:
								set action bar of loop-player to "&6%{jobs.0X2}% %{_Explore3}%€"
on any move:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		if {debug.jobs} is true:
			send action bar from "plugin &7DEBUG : %player's location%, w: %player's yaw%, p: %player's pitch%, c: %{old_chunk_o_%player%}%" to player
on eat:
	if world of player is equal to {jobs.0X41} or {jobs.0X42} or {jobs.0X43} or {jobs.0X44} or {jobs.0X45} or {jobs.0X46} or {jobs.0X47} or {jobs.0X48} or {jobs.0X49} or {jobs.0X410}:
		set {_Eat::*} to configuration section "Eat" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
		loop {_Eat::*}:
			add 1 to {_loop.number}
			set {_Eat2::*} to configuration section "Eat.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%{jobs.a.%player%}%.yml"
