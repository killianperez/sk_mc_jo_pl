						replace all ", " and " and " with "||" in {_hfuifhofh}
						if {_hfuifhofh} is "<none>":
							set {_hfuifhofh} to "&cCette partie n'est pas utiliser par le métier %arg-2%."
						format slot 33 of player with dandelion yellow named "&4&lTeindre un mouton." with lore "%{_hfuifhofh}%" to be unstealable
						clear {_estethique::*}
						clear {_hfuifhof}
								
						set {_Dye::*} to configuration section "Dye" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
						set {_loop.number} to 0
						loop {_Dye::*}:
							add 1 to {_loop.number}
							set {_Dye2::*} to configuration section "Dye.%loop-value%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
							loop {_Dye2::*}:
								set {_Dye3::*} to configuration section "Dye.%loop-value-1%.%loop-value-2%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
								set {_Dye4} to single value "Dye.%loop-value-1%.%loop-value-2%.%{_Dye3::1}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
								set {_Dye5} to single value "Dye.%loop-value-1%.%loop-value-2%.%{_Dye3::2}%" get of "plugins/killian6341_config/jobs/jobs/%arg-2%.yml"
								add "&7Item : &6%loop-value-1% &8- &7Couleur : &6%loop-value-2% &8- &7Gain: &6%{_Dye4}% Euros &8- &7EXP : &6%{_Dye5}% xp" to {_estethique::*}
						set {_hfuifhofh} to "%{_estethique::*}%"
						replace all ", " and " and " with "||" in {_hfuifhofh}
						if {_hfuifhofh} is "<none>":
							set {_hfuifhofh} to "&cCette partie n'est pas utiliser par le métier %arg-2%."
						format slot 34 of player with orange dye named "&4&lTeindre un item." with lore "%{_hfuifhofh}%" to be unstealable
						clear {_estethique::*}
						clear {_hfuifhof}
								
